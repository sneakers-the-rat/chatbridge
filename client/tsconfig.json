{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
//    "noEmit": true,
    "outDir": "../dist/",
    "jsx": "react-jsx",
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true
  },
  "include": [
    "src", "src/types.d.ts"
  ]
}
